
    <table class="highlight">
        <thead>
          <tr>
              <th data-field="id">Nombre</th>
              <th data-field="name">Rut</th>
              <th data-field="price">Equipo</th>
          </tr>
        </thead>



<% @workers.each do |worker| %>


       <tbody>
          <tr>
            <td><%= worker.nombre %></td>
            <td><%= worker.rut %></td>
            <td><%= worker.equipo %></td>
            <td ><%= link_to "Eliminar",worker, method: :delete %></td>
            
          </tr>

        </tbody>



<%end%>

      </table>
            
